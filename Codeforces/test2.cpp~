#include <bits/stdc++.h>
#include <vector>
#include <string>
using namespace std;

int main(){
	int t; cin >> t;
	string s = "yes";
	string d;
	vector<string> b;
	while(t--){
		cin >> d;
		transform(d.begin(), d.end(), d.begin(),::tolower);
		if(d == s){
			b.push_back("YES");
		}else{
			b.push_back("NO");
		}
	}
	for(auto i: b){
		cout << i << "\n";
	}
}

